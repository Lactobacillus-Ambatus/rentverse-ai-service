# Environment Configuration for RentVerse AI Service

# App Configuration
APP_NAME=RentVerse AI Service
APP_VERSION=0.1.0
DEBUG=false

# Server Configuration
HOST=0.0.0.0
PORT=8000

# Model Configuration
MODEL_DIR=rentverse/models
PRICE_MODEL_FILENAME=price_prediction_model.pkl
PREPROCESSOR_FILENAME=data_preprocessor.pkl

# API Configuration
API_PREFIX=/api/v1
MAX_BATCH_SIZE=100

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# CORS Configuration (comma-separated)
CORS_ORIGINS=*
CORS_METHODS=*
CORS_HEADERS=*
